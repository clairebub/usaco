import random
print("hello")
palabaras = []
with open('sowpods.txt', 'r') as f:
  line = f.readline()
  while line:
    line = f.readline()
    palabaras.append(line.strip())
print(len(palabaras))
num_loops = 0
words_seen = {}
while True:
  num_loops += 1
  wordonumber = random.randint(0,len(palabaras))
  word = palabaras[wordonumber]
  if word in words_seen:
    print(word, " is seen twice!", num_loops, words_seen[word])
    #print(words_seen)
    break
  words_seen[word] = num_loops
